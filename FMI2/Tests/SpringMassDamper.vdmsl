			mk_SimpleType(0, "Modelica.SIunits.AngularVelocity",	mk_RealType(nil, nil, nil, nil)),
			mk_SimpleType(0, "Modelica.SIunits.Angle",				mk_RealType(nil, nil, nil, nil))
